PROGRAM circle
	#Incorrect logic operator
	#Variable declarations
	FLOAT r, a, p, b; 	
	#Write a prompt
	WRITE "Please enter two radii values for a circle";
	r = 8;
	p = 0;
	a = 0;
	INT i, j;
	j = -1;
	IF  (r < 5)
	  a = (3.14) * r + r;
	IF (a > 100)
		p = 2 * 3.14 * b; 
	#Display the results
	WRITE "The output results are " , r, p, b;
END PROGRAM 
